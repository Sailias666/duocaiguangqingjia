

<view class='page-container'>
  <view class='title-container'>
    <view class='title-content'>
      <text class='title-font'>{{data[id-1].name}}</text>
    </view>
  </view>

  <view class='deadline-container'>
    <view class='deadline-content'>
      <text class='deadline-text'> 有效期：{{data[id].tov}}</text>
    </view>
  </view>

  <view class='border-container'>
    <view class='border'></view>
  </view>


  <view class='content-container'>
    <view class='content-content'>
      <text class='content-font'>{{data[id-1].desc}}</text>
    </view>
  </view>

  <view class='button-container'>
    <button  type='primary' class='button' bindtap='baoming'>我要报名</button>
  </view>
</view>
